import json

# Load the JSON file
with open('us_student_dataset.json', 'r') as file:
    data = json.load(file)

# Extract the list of students
students = data['students']

# Display full dataset
print("Full Dataset:\n", students)

# Example: Accessing individual elements
print("\nFirst Student Name:", students[0]['name'])
print("First Student Course:", students[0]['course'])

# Example: Loop through students and print their GPA
print("\nStudents and their GPA:")

for student in students:
    print(f"{student['name']} ({student['course']}): GPA = {student['GPA']}")
